      <ProgressIndicator prefWidth="30.0">
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin>
      </ProgressIndicator>
      <Label id="title-label" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Title" HBox.hgrow="ALWAYS">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
   </children>
   <padding>
      <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
   </padding>
</HBox>
